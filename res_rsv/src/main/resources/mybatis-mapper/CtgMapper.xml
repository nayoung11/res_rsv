<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="CtgMapper">
	<select id="selectAll" resultType="CtgVO">
		SELECT
			`CTG_NO`,
			`CTG_NM`,
			`ORDER`
		FROM 
			`TB_CTG`
	</select>
</mapper>